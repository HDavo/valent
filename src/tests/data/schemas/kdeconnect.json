{
    "$schema": "http://json-schema.org/schema#",
    "$id": "https://andyholmes.ca/kdeconnect-schema/packet",
    "required": [
        "id",
        "type",
        "body"
    ],
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "default": 0
        },
        "type": {
            "type": "string",
            "pattern": "kdeconnect.photo.request"
        },
        "body": {
            "type": "object",
            "default": {}
        }
    }
}
